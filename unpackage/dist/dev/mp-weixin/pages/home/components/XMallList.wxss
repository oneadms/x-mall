
.pro-item {
  background-color: white;
  overflow: hidden;
  text-overflow: ellipsis;
}
.pro-container {
  flex-wrap: wrap;
  display: flex;
  padding: 10px;
  margin: auto auto;
  text-align: center;
  justify-content: center;
  align-items: flex-start;
}
.pro-list {
  padding: 3px;
  width: 30%;
}
.pro-price {
  font-size: 0.8em;
  color: red;

  white-space: nowrap;
}
.pro-name {
  padding: 10px;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.pro-img {
  width: 100px;
  height: 100px;
}


